

# These are the pin assignments for the Altera Cyclone IV board
set_location_assignment PIN_84 -to led[3]
set_location_assignment PIN_85 -to led[2]
set_location_assignment PIN_86 -to led[1]
set_location_assignment PIN_87 -to led[0]
set_location_assignment PIN_23 -to sys_clk
set_location_assignment PIN_88 -to key[0]
set_location_assignment PIN_89 -to key[1]
set_location_assignment PIN_90 -to key[2]
set_location_assignment PIN_91 -to key[3]

set_location_assignment PIN_128 -to ssegment[0]
set_location_assignment PIN_121 -to ssegment[1]
set_location_assignment PIN_125 -to ssegment[2]
set_location_assignment PIN_129 -to ssegment[3]
set_location_assignment PIN_132 -to ssegment[4]
set_location_assignment PIN_126 -to ssegment[5]
set_location_assignment PIN_124 -to ssegment[6]
set_location_assignment PIN_127 -to ssegment[7]

set_location_assignment PIN_133 -to scathod[3]
set_location_assignment PIN_135 -to scathod[2]
set_location_assignment PIN_136 -to scathod[1]
set_location_assignment PIN_137 -to scathod[0]

set_location_assignment PIN_114 -to UART_RX
set_location_assignment PIN_115 -to UART_TX
